        </dependency>
        <dependency>
            <artifactId>go-through-common</artifactId>
            <groupId>com.future94</groupId>
            <version>1.0-SNAPSHOT</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>com.future94</groupId>
            <artifactId>go-through-protocol</artifactId>
            <version>1.0-SNAPSHOT</version>
            <scope>compile</scope>
        </dependency>

    </dependencies>


